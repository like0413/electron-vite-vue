{
  "name": "tingkelai",
  "version": "0.0.21",
  "main": "dist-electron/main/index.js",
  "description": "听客来客户端",
  "author": "听客来",
  "keywords": [
    "tingkelai"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "cross-env APP_URL=https://alpha.tingkelai.com/tingkelai vite",
    "preview": "vite preview",
    "build:alpha": "vue-tsc --noEmit && vite build && cross-env APP_URL=https://alpha.tingkelai.com/tingkelai electron-builder",
    "build:beta": "vue-tsc --noEmit && vite build && cross-env APP_URL=https://beta.tingkelai.com/tingkelai electron-builder",
    "build:prod": "vue-tsc --noEmit && vite build && cross-env APP_URL=https://www.tingkelai.com/tingkelai electron-builder",
    "release:alpha": "release-it --preRelease=alpha",
    "release:beta": "release-it --preRelease=beta",
    "release:prod": "release-it"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^8.0.1",
    "@vitejs/plugin-vue": "^5.0.1",
    "cross-env": "^7.0.3",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "electron-updater": "^6.1.8",
    "release-it": "^17.1.1",
    "sass": "^1.71.1",
    "tree-kill": "^1.2.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.4.1",
    "vue-tsc": "^1.8.27",
    "@ant-design/icons-vue": "^7.0.1",
    "address": "^2.0.2",
    "ant-design-vue": "4.x",
    "electron-store": "^8.2.0",
    "vue-router": "^4.3.0"
  }
}
