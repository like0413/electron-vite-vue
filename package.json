{
  "name": "tingkelai",
  "version": "0.0.15",
  "main": "dist-electron/main/index.js",
  "description": "听客来客户端",
  "author": "听客来",
  "keywords": [
    "tingkelai"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "vite --mode alpha",
    "build:alpha": "vue-tsc --noEmit && vite build --mode alpha && electron-builder",
    "build:beta": "vue-tsc --noEmit && vite build --mode beta && electron-builder",
    "build:prod": "vue-tsc --noEmit && vite build --mode prod && electron-builder",
    "preview": "vite preview",
    "release": "release-it"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^8.0.1",
    "@vitejs/plugin-vue": "^5.0.1",
    "dotenv": "^16.4.5",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "electron-log": "^5.1.1",
    "electron-updater": "^6.1.8",
    "release-it": "^17.1.1",
    "standard-version": "^9.5.0",
    "tree-kill": "^1.2.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.4.1",
    "vue-tsc": "^1.8.27"
  }
}
