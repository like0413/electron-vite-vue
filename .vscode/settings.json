{
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.tsc.autoDetect": "off",
  "json.schemas": [
    {
      "fileMatch": ["/*electron-builder.json5", "/*electron-builder.json"],
      "url": "https://json.schemastore.org/electron-builder"
    }
  ],
  "files.exclude": {
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    "**/Thumbs.db": false
  },
  "explorerExclude.backup": {}
}
